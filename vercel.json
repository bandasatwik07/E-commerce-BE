{
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"build/**",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/products",
            "use":"index.js"
        },
        {
            "src":"/products/(.*)",
            "use":"index.js"
        },
        {
            "src":"/categorys",
            "use":"index.js"
        },
        {
            "src":"/categorys/(.*)",
            "use":"index.js"
        },
        {
            "src":"/brands",
            "use":"index.js"
        },
        {
            "src":"/brands/(.*)",
            "use":"index.js"
        },
        {
            "src":"/users",
            "use":"index.js"
        },
        {
            "src":"/users/(.*)",
            "use":"index.js"
        },
        {
            "src":"/cart",
            "use":"index.js"
        },
        {
            "src":"/cart/(.*)",
            "use":"index.js"
        },
        {
            "src":"/auth",
            "use":"index.js"
        },
        {
            "src":"/auth/(.*)",
            "use":"index.js"
        },
        {
            "src":"/orders",
            "use":"index.js"
        },
        {
            "src":"/orders/(.*)",
            "use":"index.js"
        },
        {
            "src":"/webhook",
            "use":"index.js"
        },
        {
            "src":"/create-payment-intent",
            "use":"index.js"
        },
        {
            "src":"/",
            "use":"build/index.html"
        },
        {
            "src":"/(.+)",
            "use":"build/$1"
        }
    ]
}
server.use('/brands', isAuth(), brandsRouter.router);
server.use('/users', isAuth(), usersRouter.router);
server.use('/auth', authRouter.router);
server.use('/cart', isAuth(), cartRouter.router);
server.use('/orders', isAuth(), ordersRouter.router);